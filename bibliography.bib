@article{loweSmoothedParticleHydrodynamics2022,
  title = {Smoothed {{Particle Hydrodynamics}} Simulations of Reef Surf Zone Processes Driven by Plunging Irregular Waves},
  author = {Lowe, R. J. and Altomare, C. and Buckley, M. L. and {da Silva}, R. F. and Hansen, J. E. and Rijnsdorp, D. P. and Dom{\'i}nguez, J. M. and Crespo, A. J. C.},
