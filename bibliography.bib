@article{yangNumericalFlumeWaves2023,
  title = {A Numerical Flume for Waves on Variable Sheared Currents Using Smoothed Particle Hydrodynamics ({{SPH}}) with Open Boundaries},
  author = {Yang, Yong and Draycott, Samuel and Stansby, Peter K. and Rogers, Benedict D.},
