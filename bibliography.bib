  copyright = {2019 The Author(s), under exclusive licence to Springer Nature Limited},
  langid = {english},
  keywords = {Cell lineage,Computational biology and bioinformatics},
  file = {C:\Users\david\Zotero\storage\23Z9SB8Q\Nowotschin et al. - 2019 - The emergent landscape of the mouse gut endoderm a.pdf}
